
CREATE DATABASE ALIMCONFIANCE_yannis;
\c ALIMCONFIANCE_yannis;

CREATE TABLE ETABLISSEMENT (
  siret VARCHAR(42),
  nom_etablissment VARCHAR(42),
  adresse VARCHAR(42),
  agrement VARCHAR(42),
   VARCHAR(42),
  PRIMARY KEY (id_etablissement)
);

CREATE TABLE INSPECTION (
  id_inspection VARCHAR(42),
  date_inspection VARCHAR(42),
  synthese_eval VARCHAR(42),
  activite VARCHAR(42),
  siret VARCHAR(42),
  PRIMARY KEY (id_inspection)
);

ALTER TABLE INSPECTION ADD FOREIGN KEY (id_etablissement) REFERENCES ETABLISSEMENT (id_etablissement);

