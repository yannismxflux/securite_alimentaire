
CREATE TABLE ETABLISSEMENT (
  erro1 VARCHAR(500),
  id_etablissement VARCHAR(500),
  name_etablissement VARCHAR(500),
  adresse_etablissement VARCHAR(500),
  code_postal VARCHAR(500),
  nom_commune VARCHAR(500),
  name_type_etablissement VARCHAR(500),
  geores_etablissement VARCHAR(500),
  PRIMARY KEY (id_etablissement)
);

CREATE TABLE INSPECTION (
  erro1 VARCHAR(500),
  num_synthese_eval_sanitaire VARCHAR(500),
  desc_synthese_eval_sanitaire VARCHAR(500),
  date_inspection_restaurant VARCHAR(500),
  id_etablissement VARCHAR(500),
  PRIMARY KEY (num_synthese_eval_sanitaire)
);

ALTER TABLE INSPECTION ADD FOREIGN KEY (id_etablissement) REFERENCES ETABLISSEMENT (id_etablissement);

